<!DOCTYPE html>
<html>

<head>
    <title>
        <%= htmlWebpackPlugin.options.title %>
    </title>

    <base target="_self">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta charset="UTF-8">

    <meta name="viewport" content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no">
    <meta name="format-detection" content="telephone=no,email=no,address=no">
    <meta name="format-detection" content="email=no">
    <meta name="format-detection" content="address=no">
    <meta name="format-detection" content="telephone=no">
    <meta name="HandheldFriendly" content="true">
    <meta name="mobile-web-app-capable" content="yes">

    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
    <link rel="manifest" href="/manifest.json">
    <meta name="theme-color" content="#000000" />

    <!-- IE/Edge -->
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

    <!-- iOS -->
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">

    <% if (!htmlWebpackPlugin.options.__DEV__) { %>
        <script type="text/javascript" src="{{ SRC:critical }}"></script>
    <% } %>
    <% if (htmlWebpackPlugin.options.__DEV__) { %>
        <script type="text/javascript" src="//localhost:3001/dist/web.critical.js"></script>
    <% } %>
</head>

<body>
    <div id="root"></div>
    <script type="text/javascript">
        var __REDUX_STATE__ = {};
    </script>
    <% if (!htmlWebpackPlugin.options.__DEV__) { %>
    <script type="text/javascript" src="{{ SRC:client }}" defer></script>
    <% } %>
</body>

</html>